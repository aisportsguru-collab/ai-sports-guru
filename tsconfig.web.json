{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "noEmit": true,
    "skipLibCheck": true
  },
  "include": [
    "app/**/*",
    "components/**/*",
    "lib/**/*",
    "scripts/**/*",
    "pages/**/*",
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules",
    "aisportsguru-mobile",
    "**/aisportsguru-mobile/**"
  ]
}
